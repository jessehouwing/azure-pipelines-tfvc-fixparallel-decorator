{
    "manifestVersion": 1,
    "id": "azure-pipelines-tfvc-fixparallel-decorator",
    "publisher": "jessehouwing",
    "version": "1.0.0",
    "name": "Fix TFVC parallel issues on hosted pipeline",
    "description": "Azure DevOps Extension",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "logo.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
